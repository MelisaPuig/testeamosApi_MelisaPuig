<a href="/productos">Ver listado de productos</a>
<div class="cartel_bienvenido">
  <span class="cartel_bienvenido--texto">Bienvenid@ {{username}}</span>
  <button id="boton_desloguear" onclick="logout()">Desloguear</button>
</div>
<form action="/productos" method="post">
  <div class="form-group">
    <label for="title"><b>T&iacute;tulo</b></label>
    <input id="title" class="form-control" type="text" name="title" />
  </div>
  <div class="form-group">
    <label for="price"><b>Precio</b></label>
    <input id="price" class="form-control" type="number" name="price" />
  </div>
  <div class="form-group">
    <label for="thumbnail"><b>Thumbnail</b></label>
    <input id="thumbnail" class="form-control" type="text" name="thumbnail" />
  </div>
  <button class="btn btn-sucess mt-3 mb-5">Enviar</button>
</form>

<div id="websocket_products"></div>

<div id="chat">
  <div class="normalizacion_etiqueta">Porcentaje de compresi&oacute;n:
    <span id="normalizacion_porcentaje_compresion"></span>
    %</div>
  <div class="form form-group">
    <label for="chat_user_email"><b>Email</b></label>
    <input
      id="chat_user_email"
      class="form-control"
      type="text"
      name="user_email"
      required="true"
    />
    <label for="chat_user_nombre"><b>Nombre</b></label>
    <input
      id="chat_user_nombre"
      class="form-control"
      type="text"
      name="user_nombre"
      required="true"
    />
    <label for="chat_user_apellido"><b>Apellido</b></label>
    <input
      id="chat_user_apellido"
      class="form-control"
      type="text"
      name="user_apellido"
      required="true"
    />
    <label for="chat_user_edad"><b>Edad</b></label>
    <input
      id="chat_user_edad"
      class="form-control"
      type="number"
      name="user_edad"
    />
    <label for="chat_user_alias"><b>Alias</b></label>
    <input
      id="chat_user_alias"
      class="form-control"
      type="text"
      name="user_alias"
      required="true"
    />
    <label for="chat_user_avatar"><b>Avatar</b></label>
    <input
      id="chat_user_avatar"
      class="form-control"
      type="text"
      name="user_avatar"
      required="true"
    />
  </div>
  <div id="chat_messages">

  </div>
  <div class="form-group">
    <label for="chat_user_message"><b>Mensaje</b></label>
    <input
      id="chat_user_message"
      class="form-control"
      type="text"
      name="user_message"
    />
  </div>
  <button
    id="chat_send_message"
    onclick="sendUserChatMessage()"
    class="btn btn-sucess mt-3 mb-5"
  >Enviar</button>
</div>